{
  "name": "mobeigi.com-server",
  "version": "1.0.0",
  "private": true,
  "description": "Server source code for the mobeigi.com website.",
  "homepage": "https://mobeigi.com",
  "bugs": {
    "url": "https://github.com/mobeigi/mobeigi.com/issues",
    "email": "me@mobeigi.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobeigi/mobeigi.com"
  },
  "license": "UNLICENSED",
  "author": "Mo Beigi",
  "scripts": {
    "build": "ts-node build.ts",
    "clean": "rm -rf node_modules/ dist/",
    "lint": "eslint . --ext .ts",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "test": "ts-node -r tsconfig-paths/register ./spec",
    "test:dev": "nodemon --config ./spec/nodemon.json"
  },
  "nodemonConfig": {
    "exec": "ts-node -r tsconfig-paths/register ./src",
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@types/node-cron": "^2.0.4",
    "@types/xml2json": "^0.11.4",
    "ansi-align": "3.0.0",
    "ansi-regex": "^6.0.1",
    "axios": "^0.21.2",
    "body-parser": "^1.19.0",
    "command-line-args": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-promise-router": "^4.1.0",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.6",
    "jsonfile": "^6.1.0",
    "knex": "^2.4.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.35",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.0",
    "pug": "^3.0.2",
    "string-width": "5.0.1",
    "strip-ansi": "^7.0.1",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "^3.9.1",
    "@types/jsonfile": "^6.0.1",
    "@types/morgan": "^1.9.3",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.9.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "find": "^0.3.0",
    "fs-extra": "^10.0.0",
    "jasmine": "^3.9.0",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "_moduleAliases": {
    "@shared": "dist/shared",
    "@server": "dist/Server"
  }
}
