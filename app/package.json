{
  "name": "mobeigi.com",
  "version": "1.0.0",
  "author": "Mo Beigi",
  "description": "Source code for the mobeigi.com website.",
  "homepage": "https://mobeigi.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobeigi/mobeigi.com"
  },
  "bugs": {
    "url": "https://github.com/mobeigi/mobeigi.com/issues",
    "email": "me@mobeigi.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "set PORT=3001 && node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --watchAll=false --passWithNoTests",
    "clean": "rm -rf node_modules/ build/",
    "lint": "eslint src --quiet --ext .js,.jsx,.ts,.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn install --check-files && env FORCE_COLOR=1 lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn lint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "dependencies": {
    "@babel/core": "7.10.5",
    "@emotion/core": "^10.0.35",
    "@svgr/webpack": "5.4.0",
    "@types/bootstrap": "^5.0.0",
    "@types/jest": "^26.0.0",
    "@types/jquery": "^3.5.4",
    "@types/node": "^14.0.13",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-modal": "^3.10.6",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "axios": "^0.21.1",
    "babel-jest": "^26.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "bootstrap": "^4.5.3",
    "browserslist": "^4.14.5",
    "camelcase": "^6.0.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "css-loader": "3.6.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "echarts": "^5.0.0",
    "file-loader": "6.0.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "install": "^0.13.0",
    "jest": "26.1.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "26.1.0",
    "jest-watch-typeahead": "0.6.0",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "0.9.0",
    "moment": "^2.29.0",
    "moment-timezone": "^0.5.31",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.4",
    "popper.js": "^1.16.1",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-app-polyfill": "^1.0.6",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^17.0.2",
    "react-ga": "^3.0.0",
    "react-helmet-async": "^1.0.9",
    "react-is": "^17.0.2",
    "react-modal": "^3.11.2",
    "react-responsive": "^8.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-spinners": "^0.9.0",
    "react-spring": "^9.2.3",
    "resolve": "1.17.0",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "9.0.2",
    "semver": "7.3.2",
    "style-loader": "1.2.1",
    "styled-components": "^5.1.1",
    "terser-webpack-plugin": "3.0.7",
    "ts-pnp": "1.2.0",
    "typescript": "^3.9.5",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.1.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-unicode-property-regex": "^7.10.4",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@testing-library/dom": "^7.21.1",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.11",
    "@types/prop-types": "^15.7.3",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.8.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.3.2"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
